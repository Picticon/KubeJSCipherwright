{
  "type": "minecraft:stonecutting",
  "name": "Stonecutting",
  "category": "Minecraft",
  "padtop": 8,
  "padbottom": 8,
  "padleft": 8,
  "padright": 8,
  "inputs": [
    {
      "type": "ingredient",
      "gridx": 0,
      "gridy": 0,
      "single": true,
      "path": "ingredient"
    }
  ],
  "outputs": [
    {
      "type": "itemstack",
      "gridx": 3,
      "gridy": 0,
      "style": "large",
      "path": "result",
      "path:count": "count"
    }
  ],
  "illustrations": [
    {
      "type": "left_arrow",
      "gridx": 1.2,
      "gridy": 0
    },
    {
      "type": "item",
      "item": "minecraft:stonecutter",
      "scale": 1,
      "alpha": 1,
      "gridx": 6,
      "gridy": 0
    }
  ],
  "templates": [
    {
      "name": "KubeJS",
      "format": "kubejs",
      "snippets": [
        {
          "title": "Comment",
          "condition": "comment",
          "fragments": [
            "// <<description:result>>"
          ]
        },
        {
          "title": "Create",
          "fragments": [
            "event.stonecutting(<<itemstack:result>>, <<ingredient:ingredient>>);"
          ]
        },
        {
          "title": "Delete",
          "condition": "remove",
          "fragments": [
            "event.remove({id:\"<<oldid>>\"});"
          ]
        }
      ]
    },
    {
      "name": "JSON",
      "format": "json",
      "flags": "prettyjson",
      "snippets": [
        {
          "title": "JSON",
          "flags": "prettyjson",
          "fragments": [
            "{\"type\":\"<<type>>\",\"count\":<<item-count:result>>,\"ingredient\":<<ingredient:ingredient>>,\"result\":<<item-id:result>>}"
          ]
        }
      ]
    }
  ]
}
