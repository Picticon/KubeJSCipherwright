{
  "type": "create:filling",
  "name": "Filling",
  "category": "Create",
  "inputs": [
    {
      "type": "ingredient",
      "gridx": 0,
      "gridy": 1,
      "single": true,
      "path": "ingredients",
      "allowtag": true,
      "allowfluid": false
    },
    {
      "type": "ingredient",
      "gridx": 0,
      "gridy": 2,
      "single": true,
      "path": "ingredients",
      "allowtag": false,
      "allowitem": false,
      "allowfluid": true,
      "allowfluidtag": true
    }
  ],
  "outputs": [
    {
      "path": "results",
      "type": "itemstack",
      "gridx": 4,
      "gridy": 1,
      "repeat": {
        "shape": "array",
        "rows": 1,
        "cols": 1
      },
      "allowtag": false
    }
  ],
  "illustrations": [
    {
      "type": "left_arrow",
      "gridx": 2,
      "gridy": 1
    }
  ],
  "parameters": [],
  "templates": [
    {
      "name": "KubeJS Custom",
      "format": "kubejs",
      "snippets": [
        {
          "title": "Comment",
          "condition": "comment",
          "fragments": [
            "// <<description:results>>"
          ]
        },
        {
          "title": "Create",
          "fragments": [
            "event.custom(<<template:JSON>>);"
          ]
        },
        {
          "title": "Delete",
          "condition": "remove",
          "fragments": [
            "event.remove({id:\"<<oldid>>\"});"
          ]
        }
      ]
    },
    {
      "name": "JSON",
      "format": "json",
      "snippets": [
        {
          "title": "Datapack",
          "fragments": [
            "{",
            "\"type\":\"<<type>>\",",
            "\"ingredients\":<<ingredients:ingredients>>,",
            "\"results\":<<itemstacks:results>>",
            "}"
          ]
        }
      ]
    }
  ]
}