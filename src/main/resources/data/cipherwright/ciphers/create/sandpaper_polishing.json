{
  "type": "create:sandpaper_polishing",
  "name": "Sandpaper Polishing",
  "category": "Create",
  "inputs": [
    {
      "type": "ingredient",
      "single": true,
      "path": "ingredients",
      "index": 0,
      "allowfluids": false
    }
  ],
  "outputs": [
    {
      "path": "results",
      "type": "itemstack",
      "allowtag": false,
      "allowfluids": false,
      "index": 0
    }
  ],
  "illustrations": [
    {
      "type": "left_arrow",
      "gridx": 1.4,
      "gridy": 3
    },
    {
      "type": "down_arrow",
      "gridx": 0,
      "gridy": 1.5
    }
  ],
  "templates": [
    {
      "name": "KubeJS Custom",
      "format": "kubejs",
      "snippets": [
        {
          "title": "Comment",
          "condition": "comment",
          "fragments": [
            "// <<description:results>>"
          ]
        },
        {
          "title": "Create",
          "fragments": [
            "event.custom(<<template:JSON>>);"
          ]
        },
        {
          "title": "Delete",
          "condition": "remove",
          "fragments": [
            "event.remove({id:\"<<oldid>>\"});"
          ]
        }
      ]
    },
    {
      "name": "JSON",
      "format": "json",
      "flags": "prettyjson",
      "snippets": [
        {
          "title": "Datapack",
          "flags": "prettyjson",
          "fragments": [
            "{\"type\":\"<<type>>\",\"ingredients\":<<ingredients:ingredients>>,\"results\":<<itemstacks:results>>"
          ]
        },
        {
          "title": "heating",
          "condition": "parameter:keepHeldItem:equals:true",
          "fragments": [
            ",\"keepHeldItem\":<<parameter:keepHeldItem>>"
          ]
        },
        {
          "title": "closing",
          "fragments": [
            "}"
          ]
        }
      ]
    }
  ]
}