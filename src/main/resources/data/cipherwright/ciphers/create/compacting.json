{
  "type": "create:compacting",
  "name": "Compacting",
  "category": "Create",
  "inputs": [
    {
      "type": "ingredient",
      "gridx": 0,
      "gridy": 0,
      "single": true,
      "path": "ingredients",
      "repeat": {
        "shape": "array",
        "rows": 3,
        "cols": 3
      },
      "allowfluid": true,
      "allowfluidtag": true
    }
  ],
  "outputs": [
    {
      "path": "results",
      "type": "itemstack",
      "gridx": 5,
      "gridy": 1,
      "repeat": {
        "shape": "array",
        "rows": 2,
        "cols": 2
      },
      "allowtag": false,
      "allowfluid": true
    }
  ],
  "illustrations": [
    {
      "type": "left_arrow",
      "gridx": 3.2,
      "gridy": 1
    }
  ],
  "parameters": [
    {
      "path": "heatRequirement",
      "placeholder": "ticks",
      "label": "Heat",
      "type": "combo",
      "default": "none",
      "options": [
        "none",
        "heated",
        "superheated"
      ],
      "descriptions": [
        "None",
        "Heated",
        "Super Heated"
      ],
      "gridx": 2,
      "gridy": 4,
      "gridw": 5
    }
  ],
  "templates": [
    {
      "name": "KubeJS Custom",
      "format": "kubejs",
      "snippets": [
        {
          "title": "Comment",
          "condition": "comment",
          "fragments": [
            "// <<description:results>>"
          ]
        },
        {
          "title": "Create",
          "fragments": [
            "event.custom(<<template:JSON>>);"
          ]
        },
        {
          "title": "Delete",
          "condition": "remove",
          "fragments": [
            "event.remove({id:\"<<oldid>>\"});"
          ]
        }
      ]
    },
    {
      "name": "JSON",
      "format": "json",
      "snippets": [
        {
          "title": "Datapack",
          "fragments": [
            "{",
            "\"type\":\"<<type>>\",",
            "\"ingredients\":<<ingredients:ingredients>>,",
            "\"results\":<<itemstacks:results>>"
          ]
        },
        {
          "title": "heating",
          "condition": "parameter:heatRequirement:not:none",
          "fragments": [
            ",\"heatRequirement\":<<parameter:heatRequirement>>"
          ]
        },
        {
          "title": "closing",
          "fragments": [
            "}"
          ]
        }
      ]
    }
  ]
}